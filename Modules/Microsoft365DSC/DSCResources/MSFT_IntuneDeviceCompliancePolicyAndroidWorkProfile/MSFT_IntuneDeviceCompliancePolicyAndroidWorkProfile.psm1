        $SecurityRequireUpToDateSecurityProviders,

        [Parameter()]
        [System.Boolean]
        $SecurityRequireCompanyPortalAppIntegrity,

        [Parameter()]
        [System.String]
        [ValidateSet('basic', 'hardwareBacked')]
        $SecurityRequiredAndroidSafetyNetEvaluationType,

