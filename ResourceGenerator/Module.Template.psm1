    param
    (<#ResourceGenerator
        #region resource generator code
<ParameterBlock>
<AssignmentsParam>
        #endregion ResourceGenerator#>

        [Parameter(Mandatory = $true)]
        [System.String]
        [ValidateSet('Absent', 'Present')]
        $Ensure = $true,

        [Parameter()]
        [System.Management.Automation.PSCredential]
